{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@viapip/eslint-config",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@4.4.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": "",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll --minify",
    "typecheck": "tsc --noEmit",
    "dev": "esno src/index.ts",
    "start": "node --experimental-modules --experimental-import-meta-resolve dist/index.js",
    "release": "yarn publish",
    "lint": "eslint --flag unstable_ts_config .",
    "clean": "yarn exec rm -rf dist"
  },
  "dependencies": {
    "@antfu/eslint-config": "^3.5.1",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "esm": "^3.2.25",
    "esno": "^4.7.0",
    "jiti": "^1.21.6"
  },
  "devDependencies": {
    "@antfu/utils": "^0.7.10",
    "@types/esm": "^3.2.2",
    "@types/node": "^22.5.4",
    "@yarnpkg/doctor": "^4.0.2",
    "consola": "^3.2.3",
    "eslint": "^9.10.0",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^15.2.10",
    "pkgroll": "^2.4.2",
    "simple-git-hooks": "^2.11.1",
    "tslib": "^2.7.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint --flag unstable_ts_config --fix"
  },
  "dependenciesMeta": {
    "@rollup/plugin-typescript@11.1.1": {
      "unplugged": true
    },
    "rollup-plugin-esbuild@5.0.0": {
      "unplugged": true
    }
  }
}
